{% block content %} 
<script>
function bigIntPi(precision) {
    const piNumerator = BigInt(355);
    const piDenominator = BigInt(113);
    return (piNumerator * precision) / piDenominator;
}

function get_prime() {
    const twoTo4096 = 2n ** 4096n;
    const twoTo4032 = 2n ** 4032n;
    const twoTo64 = 2n ** 64n;
    const twoTo3966 = 2n ** 3966n;

    const piMultiplied = bigIntPi(twoTo3966);

    const result = twoTo4096 - twoTo4032 - 1n + twoTo64 * (piMultiplied + 240904n);

    return result;
}
</script>

